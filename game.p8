pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
player={
 pos={x=64,y=64},
 flipped=false,
 vel={x=0,y=0},
	sprites={
  walk={1,2,3,2},
 },
	keys={
	 l=0,r=1,j=4,s=5
	},
	thrust={x=1.5,y=10},
 animdist=0,
}

-- returns 1 if x is positive, -1 if x is negative
function sign(x)
 return x/abs(x)
end

-- returns 1 if x is positive, -1 if x is negative
function sign0(x)
 if (x==0) return x
 return sign(x)
end

function map_open(x, y)
 local solid = false
 for x=x,x+7 do
  for y=y,y+7 do
   local n=mget(flr(x/8),flr(y/8))
   solid = solid or fget(n, 0)
  end
 end
 return not solid
end

bullets = {}
function make_bullet(x, y, flipped)
 bullet = {
  pos={x=x, y=y}, flipped=flipped,
  speed=0, thrust=1,
  sprites={16,17}
 }
 function bullet:sprite()
  return self.sprites[1+flr(rnd(2))]
 end
 function bullet:update()
  local f=1
  if (self.flipped) f=-1
  self.pos.x+=self.speed*f
  self.speed+=self.thrust
  if self.pos.x<cam.x-8 or self.pos.x>cam.x+128 then
   del(bullets, self)
  end
 end
 function bullet:draw()
   spr(self:sprite(), self.pos.x,self.pos.y, 1,1, self.flipped)
 end
 add(bullets, bullet)
 return bullet
end

function player:update()
 local dx=0
 if btn(self.keys.l) then
  dx=-self.thrust.x
  self.flipped=true
 end
 if btnp(self.keys.s) then
  make_bullet(self.pos.x, self.pos.y, self.flipped)
 end
 if btn(self.keys.r) then
  dx=self.thrust.x
  self.flipped=false
 end
 self.vel.x=dx
 self.animdist+=abs(dx)
 if btnp(self.keys.j) then
  self.vel.y-=self.thrust.y
 end

 for d in all{'x','y'} do
  local p = {x=self.pos.x, y=self.pos.y}
  local from = self.pos[d]
  local to = from+self.vel[d]
  for v = from,to,sign(self.vel[d]) do
   p[d] = v
   if not map_open(p.x, p.y) then
    self.vel[d]=0
    break
   else
    self.pos[d]=v
   end
  end
 end
end

function player:sprite()
  return self.sprites.walk[1+(flr(self.animdist/8) % #self.sprites.walk)]
end

function player:draw()
 spr(self:sprite(), self.pos.x,self.pos.y, 1,1, self.flipped)
end

function _update60()
 local g=player.pos.x/64+0.5
 player.vel.y+=g
 player:update()
 for bullet in all(bullets) do
  bullet:update()
 end
end

cam={x=0,y=0}
function _draw()
 cls()
 cam.x=player.pos.x-64
 if player.pos.y<cam.y+8 then
  cam.y=player.pos.y-16
 elseif player.pos.y>cam.y+120 then
  cam.y=player.pos.y-112
 end

 camera(cam.x, cam.y)
 map(0,0, 0,0)
 for bullet in all(bullets) do
  bullet:draw()
 end
 player:draw()
end

